{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "src/app/[transport]/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/cron/sync/route.ts": {
      "maxDuration": 60
    }
  },
  "crons": [
    {
      "path": "/api/cron/sync",
      "schedule": "0 3 * * *"
    }
  ]
}
